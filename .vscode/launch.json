{
    "version": "0.2.0",
    "configurations": [        
        {
            "name": "SampleConsole",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/src/SampleConsole/bin/Debug/netcoreapp2.0/SampleConsole.dll",
            "args": [],
            "cwd": "${workspaceRoot}/src/SampleConsole",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "logging": {
                "exceptions": true,
                "moduleLoad": false,
                "programOutput": true,
                "engineLogging": false,
                "browserStdOut": false
            }
        },
        {
            "name": "ModelGenerator",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/src/NetCoreForce.ModelGenerator/bin/Debug/netcoreapp2.0/dotnet-modelgenerator.dll",
            "args": ["generate"],
            "cwd": "${workspaceRoot}/src/NetCoreForce.ModelGenerator",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}